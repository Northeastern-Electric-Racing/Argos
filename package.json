{
  "name": "argos",
  "version": "1.0.0",
  "scripts": {
    "lint": "npx eslint .",
    "test": "cd scylla-server && npm run test",
    "prettier-check": "npx prettier --check .",
    "install-dependencies": "npm install && cd scylla-server && npm install && cd ../angular-client && npm install",
    "build": "cd scylla-server && npm run build && cd ../angular-client && npm run build",
    "start:production": "cd scylla-server && npm run start:production && cd ../angular-client && npm run start:production",
    "prisma:generate": "cd scylla-server && npm run prisma:generate",
    "prisma:migrate": "cd scylla-server && npm run prisma:migrate",
    "prisma:migrate:prod": "cd scylla-server && npm run prisma:migrate:prod",
    "build:proto": "cd scylla-server && npm run build:proto"
  },
  "author": "Northeastern Electric Racing",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.3"
  },
  "dependencies": {
    "mapbox-gl": "^3.1.2"
  }
}
