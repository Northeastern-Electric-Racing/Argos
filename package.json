{
  "name": "argos",
  "version": "1.0.0",
  "scripts": {
    "lint": "npx eslint .",
    "prettier-check": "npx prettier --check .",
    "install-dependencies": "npm install && cd ../angular-client && npm install",
    "build": "cd scylla-server && npm run build && cd ../angular-client && npm run build",
    "prisma:generate": "cd scylla-server && cargo prisma generate",
    "prisma:seed": "cd scylla && cargo prisma-seed",
    "start": "concurrently \"npm run start --prefix angular-client\" \"cd scylla-server && cargo run"
  },
  "author": "Northeastern Electric Racing",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "3.2.5"
  }
}
